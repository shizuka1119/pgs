<div class="main users-index">
  <div class="container">
    <h1 class="users-heading">User Index</h1>
    <% @users.each do |user| %>
      <div class="users-index-item">
        <div class="user-left">
          <% if user.profile_image.attached? %>
            <%= image_tag user.profile_image %>
          <% end %>
        </div>
        <div class="user-right">
        ã€€ <%= link_to user.name, user_path(user.id) %>
        </div>
      </div>
    <% end %>
  </div>
</div>
