<div class="main items-index">
  <div class="container">
    <h1 class="users-heading">Note Index</h1>
  	<% @items.each do |item| %>
      <div class="items-index-item">
        <div class="item-left">
        </div>
        <div class="item-right">
          <div class="item-user-name">
           <%= link_to item.user.name, "/users/#{item.user.id}"%>
          </div>
           <%= link_to item.title, item_path(item) %>
        </div>
      </div>
    <% end %>
  </div>
</div>
